{
  "name": "@aureooms/js-hash",
  "description": "Hashing algorithms for JavaScript",
  "version": "1.0.1",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "ava": {
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "files": [
      "test/src/**/*"
    ],
    "timeout": "1m",
    "concurrency": 8
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-hash/issues"
  },
  "dependencies": {
    "@aureooms/js-array": "^4.0.0",
    "@aureooms/js-error": "^3.0.0",
    "@aureooms/js-uint32": "^1.0.1",
    "@aureooms/js-uint64": "^2.0.1"
  },
  "devDependencies": {
    "@aureooms/js-array": "4.0.0",
    "@aureooms/js-codec-ascii": "1.0.1",
    "@aureooms/js-itertools": "3.4.0",
    "ava": "3.8.1",
    "babel-cli": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "codeclimate-test-reporter": "0.5.1",
    "coveralls": "2.13.3",
    "esdoc": "1.1.0",
    "nyc": "11.9.0",
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.9.6",
    "@babel/register": "7.9.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "6.2.1"
  },
  "homepage": "http://aureooms.github.io/js-hash",
  "keywords": [
    "hash",
    "hashing",
    "javascript",
    "js"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/aureooms/js-hash.git",
    "type": "git"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "prepublish": "npm run build",
    "test": "ava",
    "esdoc": "esdoc"
  }
}
