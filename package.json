{
  "name": "@aureooms/js-hash",
  "description": "Hashing algorithms for JavaScript",
  "version": "2.0.0",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "ava": {
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "files": [
      "test/src/**/*"
    ],
    "timeout": "1m",
    "concurrency": 8
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-hash/issues"
  },
  "dependencies": {
    "@aureooms/js-array": "^4.0.0",
    "@aureooms/js-error": "^4.0.0",
    "@aureooms/js-uint32": "^1.0.1",
    "@aureooms/js-uint64": "^2.0.1"
  },
  "devDependencies": {
    "@aureooms/js-array": "4.0.0",
    "@aureooms/js-codec-ascii": "2.0.1",
    "@aureooms/js-itertools": "4.0.0",
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.0",
    "@babel/polyfill": "7.10.4",
    "@babel/preset-env": "7.11.0",
    "@babel/register": "7.10.5",
    "ava": "3.11.0",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "6.3.2",
    "nyc": "15.1.0"
  },
  "files": [
    "lib"
  ],
  "homepage": "http://aureooms.github.io/js-hash",
  "keywords": [
    "hash",
    "hashing",
    "javascript",
    "js"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/aureooms/js-hash.git",
    "type": "git"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava"
  }
}
